{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "dev": {
      "dependsOn": ["^dev"],
      "persistent": false,
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"]
    },
    "auth:schema:generate": {
      "cache": false,
      "interactive": true
    },
    "typecheck": {
      "dependsOn": ["^build"],
      "outputs": [".cache/tsbuildinfo.json"]
    },
    "push": {
      "env": ["DB_POSTGRES_URL"],
      "cache": false,
      "interactive": true
    },
    "start": {
      "dependsOn": ["^build"]
    },
    "studio": {
      "cache": false,
      "persistent": true
    },
    "ui-add": {
      "cache": false,
      "interactive": true
    },
    "env:copy-example": {
      "cache": false
    },
    "env:remove": {
      "cache": false
    },
    "apply": {
      "cache": false,
      "interactive": true
    },
    "types": {
      "cache": false
    },
    "secrets:add": {
      "cache": false,
      "interactive": true
    },
    "secrets:list": {
      "cache": false
    }
  },
  "globalEnv": [],
  "globalPassThroughEnv": ["NODE_ENV", "CI", "npm_lifecycle_event"]
}
